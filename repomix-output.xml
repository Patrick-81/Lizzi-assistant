// src/core/long-term-memory.ts
import { promises as fs } from 'fs';
import path from 'path';
import { fileURLToPath } from 'url';

const __filename = fileURLToPath(import.meta.url);
const __dirname = path.dirname(__filename);

export interface Fact {
  id: string;
  key: string;
  value: string;
  context?: string;
  createdAt: string;
  updatedAt: string;
}

export class LongTermMemory {
  private memoryPath: string;
  private facts: Map<string, Fact>;

  constructor() {
    const projectRoot = path.join(__dirname, '../..');
    this.memoryPath = path.join(projectRoot, 'data', 'memories.json');
    this.facts = new Map();
  }

  async initialize() {
    // Crée le dossier data s'il n'existe pas
    const dataDir = path.dirname(this.memoryPath);
    try {
      await fs.mkdir(dataDir, { recursive: true });
    } catch (error) {
      // Le dossier existe déjà
    }

    // Charge les souvenirs existants
    try {
      const data = await fs.readFile(this.memoryPath, 'utf-8');
      const factsArray: Fact[] = JSON.parse(data);
      factsArray.forEach(fact => {
        this.facts.set(fact.id, fact);
      });
      console.log(`💾 ${this.facts.size} souvenirs chargés`);
    } catch (error) {
      // Fichier n'existe pas encore, on le créera au premier save
      console.log('💾 Nouvelle mémoire créée');
      await this.saveToFile();
    }
  }

  private async saveToFile() {
    const factsArray = Array.from(this.facts.values());
    await fs.writeFile(
      this.memoryPath,
      JSON.stringify(factsArray, null, 2),
      'utf-8'
    );
  }

  async save(key: string, value: string, context?: string): Promise<Fact> {
    const now = new Date().toISOString();

    // Cherche si un fait existe déjà avec cette clé
    let fact = Array.from(this.facts.values()).find(f => f.key === key);

    if (fact) {
      // Mise à jour
      fact.value = value;
      fact.context = context;
      fact.updatedAt = now;
    } else {
      // Création
      fact = {
        id: `fact_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
        key,
        value,
        context,
        createdAt: now,
        updatedAt: now
      };
      this.facts.set(fact.id, fact);
    }

    await this.saveToFile();
    return fact;
  }

  async get(id: string): Promise<Fact | null> {
    return this.facts.get(id) || null;
  }

  async search(query: string): Promise<Fact[]> {
    const lowerQuery = query.toLowerCase();
    return Array.from(this.facts.values()).filter(fact =>
      fact.key.toLowerCase().includes(lowerQuery) ||
      fact.value.toLowerCase().includes(lowerQuery) ||
      (fact.context && fact.context.toLowerCase().includes(lowerQuery))
    );
  }

  async getAll(): Promise<Fact[]> {
    return Array.from(this.facts.values()).sort((a, b) =>
      new Date(b.updatedAt).getTime() - new Date(a.updatedAt).getTime()
    );
  }

  async update(id: string, key: string, value: string): Promise<Fact | null> {
    const fact = this.facts.get(id);
    if (!fact) return null;

    fact.key = key;
    fact.value = value;
    fact.updatedAt = new Date().toISOString();

    await this.saveToFile();
    return fact;
  }

  async delete(id: string): Promise<boolean> {
    const existed = this.facts.delete(id);
    if (existed) {
      await this.saveToFile();
    }
    return existed;
  }

  async getSummary(): Promise<string> {
    const facts = await this.getAll();

    if (facts.length === 0) {
      return "Je n'ai aucun souvenir enregistré pour le moment.";
    }

    let summary = "Voici ce dont je me souviens :\n\n";
    facts.forEach(fact => {
      summary += `- **${fact.key}** : ${fact.value}\n`;
    });

    return summary;
  }
}
